<!DOCTYPE html>
<html>
<head>
	<title>Canvas Squares</title>
</head>
<body>
<div style="position: relative;">
 <canvas id="outsideCanvas" width="300" height="300" 
   style="position: absolute; left: 0; top: 0; z-index: 0;"></canvas>
 <canvas id="insideCanvas" width="300" height="300" 
   style="position: absolute; left: 0; top: 0; z-index: 1;"></canvas>
</div>
</canvas>
</body>
<script>
	var outsideCanvas = document.getElementById('outsideCanvas')
	var outsideContext = outsideCanvas.getContext("2d");
	var insideCanvas = document.getElementById('insideCanvas')
	var insideContext = insideCanvas.getContext("2d");
	outsideContext.fillStyle="blue";
	outsideContext.fillRect(0,0,300,300)
	insideContext.fillStyle="yellow"
	insideContext.fillRect(75,75,150,150)
</script>
</html>